# AgentOS Environment Variables Configuration
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
# Required for browser agent and main language model functionality
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Configuration  
# Required for terminal developer agent functionality
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini API Configuration
# Required for video processing and generation capabilities
# Get your API key from: https://console.cloud.google.com/apis/credentials
GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud Configuration
# Required for video generation with Veo 3.0 model
# Project ID from your Google Cloud Console
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id

# Google Cloud region/location for video generation
# Common values: us-central1, us-east1, europe-west1
GOOGLE_CLOUD_LOCATION=us-central1

# Workspace Configuration
# Directory for storing generated artifacts and files
# If not set, defaults to "artifacts"
ARTIFACTS_DIR=artifacts

# Optional: Custom workspace directory
# Used in some test scenarios
WORKSPACE_DIR=workspace 